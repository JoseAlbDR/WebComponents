{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-empty": "warn",
    "semi": "off",
    "@typescript-eslint/semi": "off",
    "@typescript-eslint/no-non-null-assertion": "off",
    "@typescript-eslint/space-before-function-paren": "off"
  }
}
